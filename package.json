{
  "name": "service-worker-react-starter",
  "version": "1.0.0",
  "description": "Serve dynamic HTML with React inside a Service Worker",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --config .prettierrc \"**/*.{ts,tsx}\" --write",
    "build": "parcel build",
    "serve": "http-server dist -p 8080"
  },
  "app": "dist/app.js",
  "client": "dist/client.js",
  "targets": {
    "app": {
      "source": "./src/app/index.tsx",
      "distDir": "./dist",
      "scopeHoist": false
    },
    "client": {
      "source": "./src/client/index.tsx",
      "distDir": "./dist"
    }
  },
  "author": "Michael Puckett <michael@puckett.contact>",
  "license": "MIT",
  "dependencies": {
    "@express-worker/app": "^1.3.0",
    "@types/marked": "^6.0.0",
    "marked": "^15.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "^2.10.3",
    "@types/debounce": "^1.2.4",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "http-server": "^14.1.1",
    "parcel": "^2.10.3",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
