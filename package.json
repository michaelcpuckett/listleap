{
  "name": "swotion",
  "version": "1.0.0",
  "description": "Local Notion clone with Service Workers",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --config .prettierrc \"**/*.{ts,tsx}\" --write",
    "build": "parcel build --no-scope-hoist"
  },
  "app": "dist/app.js",
  "client": "dist/client.js",
  "targets": {
    "app": {
      "source": "./src/app/index.tsx",
      "distDir": "./dist"
    },
    "client": {
      "source": "./src/client/index.ts",
      "distDir": "./dist"
    }
  },
  "author": "Michael Puckett <michael@puckett.contact>",
  "license": "ISC",
  "dependencies": {
    "@express-worker/app": "^0.0.32",
    "debounce": "^1.2.1",
    "escape-string-regexp": "^5.0.0",
    "idb": "^7.1.1",
    "lexorank": "^1.0.5",
    "path-to-regexp": "^6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1",
    "yjs": "^13.6.8"
  },
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "^2.10.3",
    "@types/debounce": "^1.2.4",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "parcel": "^2.10.3",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  }
}
